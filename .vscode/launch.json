{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: InterviewReportApp Console Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/src/InterviewReportApp.Console/InterviewReportApp.Console.csproj"
        },
        {
            "name": "C#: InterviewReportApp Web Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/InterviewReportApp.Web/bin/Debug/netcoreapp3.1/InterviewReportApp.Web.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/InterviewReportApp.Web",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/src/InterviewReportApp.Web/Views"
            }
        }
    ]
}